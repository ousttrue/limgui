{
    "cmake.generator": "Ninja",
    "cmake.configureArgs": [
        "-DCMAKE_EXPORT_COMPILE_COMMANDS=1",
    ],
    "cSpell.words": [
        "cmake",
        "glfw",
        "imgui"
    ],
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.preloadFileSize": 10000,
    "Lua.runtime.path": [
        "?.lua",
        "?/init.lua",
        "?/?.lua",
        "${workspaceFolder}/luajitffi/?.lua",
    ],
    "Lua.diagnostics.globals": [
        "newproxy"
    ],
    "luaTestAdapter.luaExe": "luajitffi/LuaJIT/src/luajit.exe",
    "luaTestAdapter.testGlob": "tests/*.lua",
}